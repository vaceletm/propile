<%= title "Edition de revue pour: #{@review.session.title}" %> 

<%= content_for :sub_menu do %> 
  <%= link_to 'Retour à Session', @review.session %>
<% end %>

<%= content_for :extra_info do %> 
  <div><i>Le but de la revu est d'aider à améliorer les session proposée.</i></div>
<% end %>

<%= content_for :created_and_modified do %> 
  Créé le: <%= l @review.created_at %> -- 
  Modifié le: <%= l @review.updated_at %> 
<% end %>

<%= render 'form', review: @review, :show_session=>true %>

